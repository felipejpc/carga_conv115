<p id="notice"><%= notice %></p>

<h1>Listing Mestre Documentos Fiscais</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mestre_documentos_fiscais.each do |mestre_documento_fiscal| %>
      <tr>
        <td><%= link_to 'Show', mestre_documento_fiscal %></td>
        <td><%= link_to 'Edit', edit_mestre_documento_fiscal_path(mestre_documento_fiscal) %></td>
        <td><%= link_to 'Destroy', mestre_documento_fiscal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mestre documento fiscal', new_mestre_documento_fiscal_path %>
